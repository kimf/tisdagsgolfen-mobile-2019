{
  "name": "tisdagsgolfen",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "lint": "yarn run lint:compile && yarn run lint:ts",
    "lint:compile": "tsc --skipLibCheck",
    "lint:ts": "tslint -p .",
    "graphql:schema": "apollo-codegen introspect-schema http://192.168.1.246:3000/api/graphql --output schema.json",
    "graphql:gentypes": "apollo-codegen generate src/graph/**/*.ts --schema schema.json --target typescript --addTypename --output operation-result-types.ts",
    "android": "react-native run-android",
    "android:build:dev": "cd android && ./gradlew assembleRelease",
    "android:build:prod": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "android:emulator": "emulator @Nexus_6P_API_27 -netdelay none -netspeed full",
    "ios": "react-native run-ios --simulator \"iPhone 8\"",
    "prettier": "prettier \"**/*.ts?(x)\" \"**/*.js?(x)\" \"**/*.json\" \"**/*.scss\" --write"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.3.11",
