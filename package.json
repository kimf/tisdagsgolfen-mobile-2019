{
  "name": "tisdagsgolfen",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "clean": "watchman watch-del-all && rm -rf $TMPDIR/react-*",
    "test": "jest",
    "lint": "eslint src",
    "graphql:schema":
      "apollo-codegen introspect-schema https://www.tisdagsgolfen.se/api/graphql --output schema.json",
    "graphql:gentypes":
      "apollo-codegen generate src/graph/**/*.js --schema schema.json --target typescript --output operation-result-types.ts",
    "storybook": "storybook start -p 7007",
    "android:build:dev": "cd android && ./gradlew assembleRelease",
    "android:build:prod": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "android:emulator": "emulator -avd Nexus6-android7.1.1-apiLevel25",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator \"iPhone 8\"",
    "ios:storybook": "react-native run-ios --simulator \"iPhone 8\" --scheme TGStorybook",
    "prettier": "prettier --write 'src/**/*.js'",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.7",
    "apollo-client": "^2.3.8",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "immutability-helper": "^2.7.1",
    "lodash.groupby": "^4.6.0",
    "lodash.keys": "^4.2.0",
    "lodash.pick": "^4.4.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "16.4.2",
    "react-apollo": "^2.1.11",
    "react-native": "0.50.3",
    "react-native-config": "^0.11.5",
    "react-native-device-log": "^1.0.2",
    "react-native-dom": "0.2.0",
    "react-native-onesignal": "^3.2.6",
    "react-native-segmented-view": "^2.0.0",
    "react-native-slowlog": "^1.0.2",
    "react-navigation": "^2.11.2",
    "react-style-proptype": "^3.2.1",
    "subscriptions-transport-ws": "^0.9.14"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/react-native": "^3.4.10",
    "apollo-codegen": "^0.20.2",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native": "5.0.2",
    "babylon": "^6.17.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-react-native": "^3.2.1",
    "eslint-plugin-react-perf": "^2.0.9",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "rnpm-plugin-dom": "^0.1.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
