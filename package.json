    "apollo-link-http": "^1.5.7",
    "apollo-link-logger": "^1.2.3",
    "apollo-link-state": "^0.4.2",
    "graphql": "14.0.2",
    "graphql-tag": "^2.10.0",
    "immutability-helper": "^2.9.0",
    "jsc-android": "^236355.x.x",
    "lodash.groupby": "^4.6.0",
    "lodash.keys": "^4.2.0",
    "lodash.pick": "^4.4.0",
