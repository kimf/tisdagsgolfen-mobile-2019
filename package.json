    "react": "16.6.3",
    "react-apollo": "^2.3.2",
    "react-native": "0.57.8",
    "react-native-config": "^0.11.7",
    "react-native-dom": "0.4.1",
    "react-native-gesture-handler": "^1.0.10",
    "react-native-onesignal": "3.2.11",
    "react-native-segmented-view": "^2.0.0",
    "react-native-slowlog": "^1.0.2",
    "react-navigation": "^3.0.8"
